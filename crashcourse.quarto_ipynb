        "d1[\"age\"] = 24         # Updating the age\n",
        "d1[\"email\"] = \"pgschlosser@wm.edu\"  # Adding a new key-value pair\n",
        "print(d1)  # Output: {'name': 'Schlosser', 'age': 24, 'city': 'Williamsburg', 'email': '\n",
        "\n",
